{
  "name": "proton-drive-webdav-bridge",
  "version": "0.1.0",
  "description": "WebDAV bridge for Proton Drive - access your Proton Drive files via WebDAV protocol",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "proton-drive-webdav-bridge": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "build:bin": "bun build --target=bun --compile --minify ./src/index.ts --outfile ./dist/proton-drive-webdav-bridge",
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' '*.json' '*.md'",
    "pre-commit": "bun run build:check && bun run lint:fix && bun run format",
    "test": "bun test --bail",
    "test:e2e": "bun test test/*.e2e.test.ts",
    "test:unit": "bun test --bail $(find test -name '*.test.ts' ! -name '*.e2e.test.ts')",
    "test:ci": "mkdir -p test-results && bun test --bail $(find test -name '*.test.ts' ! -name '*.e2e.test.ts') --reporter=junit --reporter-outfile=./test-results/junit.xml",
    "test:e2e:ci": "mkdir -p test-results && bun test test/*.e2e.test.ts --reporter=junit --reporter-outfile=./test-results/e2e-junit.xml",
    "gui:dev": "bun ./src/gui/server.ts --port 5173",
    "gui:build": "bun build ./src/gui/index.html --outdir ./dist/gui --target browser --minify",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "sidecar:start": "bun run src/index.ts",
    "sidecar:build": "bun build --target=bun --compile --minify ./src/index.ts --outfile ./dist/proton-drive-webdav-bridge",
    "pack:dist": "tar -czf proton-drive-webdav-bridge-${npm_package_version}.tar.gz -C dist .",
    "prepare": "husky"
  },
  "keywords": [
    "proton",
    "drive",
    "webdav",
    "bridge",
    "sync",
    "cloud",
    "storage"
  ],
  "author": "proletarius101",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/secure-system/proton/proton-drive-webdav-bridge"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@inquirer/prompts": "^8.0.1",
    "@mielo-ui/adwaita-symbolic-icons-react": "^1.0.5",
    "@mielo-ui/mielo": "^1.0.8",
    "@mielo-ui/mielo-react": "^1.0.8",
    "@mielo-ui/mielo-react-slider": "^1.0.5",
    "@napi-rs/keyring": "^1.1.6",
    "@protontech/drive-sdk": "^0.9.6",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-autostart": "~2",
    "bcryptjs": "^3.0.3",
    "commander": "^14.0.2",
    "env-paths": "^3.0.0",
    "express": "^5.0.0",
    "nephele": "^1.0.0-alpha.67",
    "openpgp": "^6.2.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "usehooks-ts": "^3.1.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@happy-dom/global-registrator": "^20.3.7",
    "@tauri-apps/cli": "^2.9.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/bun": "^1.3.5",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.7.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "build": {
    "appId": "io.gitlab.secure-system.proton-drive-webdav-bridge",
    "productName": "Proton Drive WebDAV Bridge",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility"
    },
    "win": {
      "target": "nsis"
    }
  },
  "patchedDependencies": {
    "nephele@1.0.0-alpha.67": "patches/nephele@1.0.0-alpha.67.patch"
  }
}
